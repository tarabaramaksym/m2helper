{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2023",
		"outDir": "./out",
		"lib": ["es2023"],
		"sourceMap": true,
		"rootDir": "./src",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": "./src",
		"paths": {
			"constant/*": ["constant/*"],
			"Util/*": ["util/*"],
			"type/*": ["type/*"],
			"Processor/*": ["processor/*"],
			"Builder/*": ["builder/*"],
			"Initializer/*": ["initializer/*"],
			"InitializerContent/*": ["initializer/content/*"],
			"InitializerFile/*": ["initializer/file/*"],
			"State/*": ["state/*"]
		}
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode", "out"]
}
