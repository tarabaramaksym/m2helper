{
	"compilerOptions": {
	  "module": "commonjs",
	  "target": "es2020",
	  "outDir": "./out",
	  "lib": ["es2020"],
	  "sourceMap": true,
	  "rootDir": "./src",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "baseUrl": "./src",
	  "paths": {
		"constant/*": ["constant/*"],
		"util/*": ["util/*"],
		"type/*": ["type/*"]
	  }
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode", "out"]
  }
  